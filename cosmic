local start = tick()

loadstring(game:HttpGet("https://raw.githubusercontent.com/laagginq/other/main/dhacbypass.lua", true))() --// clock ;)

local GetService = setmetatable({}, {
	__index = function(self, key)
		return game:GetService(key)
	end
})


local target = ""
target = game.Players.LocalPlayer.Name

local player = game.Players.LocalPlayer

local ScriptSettings = {
    ATM = false,
    Shoes = false,
    Hospital = false,
    Lettuce = false,
}

local Old = {
    CFrame 
}

local FraudSets = {
    Blatant = {
        Farming = {
            ATMPick = false,
        },
        Speed = {
            Enabled = false,
            Speed = 1,
        }
    }
}
local Players = GetService.Players
local LocalPlayer = Players.LocalPlayer
local Workspace = game.Workspace
local RS = GetService.RunService

function notify(text) 
    game:GetService("StarterGui"):SetCore("SendNotification", {
		Title = "CosmicWare ";
		Text = text,
        Duration = 3
	})
end

function Alive(Player)
	if Player and Player.Character and Player.Character:FindFirstChild("HumanoidRootPart") ~= nil and Player.Character:FindFirstChild("Humanoid") ~= nil and Player.Character:FindFirstChild("Head") ~= nil then
		return true
	end
	return false
end

local formatNumber = (function (n)
	n = tostring(n)
	return n:reverse():gsub("%d%d%d", "%1,"):reverse():gsub("^,", "")
end)


